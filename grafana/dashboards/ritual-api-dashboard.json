{
  "dashboard": {
    "title": "Ritual Shop API Dashboard",
    "tags": ["api", "nestjs", "prometheus"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Request Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "legendFormat": "{{method}} {{route}} ({{status_code}})",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "reqps", "label": "Requests/sec" },
          { "format": "short" }
        ]
      },
      {
        "id": 2,
        "title": "HTTP Request Duration (p95)",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{method}} {{route}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "s", "label": "Duration" },
          { "format": "short" }
        ]
      },
      {
        "id": 3,
        "title": "Active HTTP Requests",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
        "targets": [
          {
            "expr": "http_active_requests",
            "legendFormat": "{{method}} {{route}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "short", "label": "Active Requests" },
          { "format": "short" }
        ]
      },
      {
        "id": 4,
        "title": "HTTP Status Code Distribution",
        "type": "piechart",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
        "targets": [
          {
            "expr": "sum by (status_code) (rate(http_requests_total[5m]))",
            "legendFormat": "{{status_code}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "Database Query Duration (p95)",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(db_query_duration_seconds_bucket[5m]))",
            "legendFormat": "{{operation}} {{table}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "s", "label": "Duration" },
          { "format": "short" }
        ]
      },
      {
        "id": 6,
        "title": "Database Query Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
        "targets": [
          {
            "expr": "rate(db_queries_total[5m])",
            "legendFormat": "{{operation}} {{table}} ({{status}})",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "ops", "label": "Queries/sec" },
          { "format": "short" }
        ]
      },
      {
        "id": 7,
        "title": "Cache Hit/Miss Ratio",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m])",
            "legendFormat": "Hits",
            "refId": "A"
          },
          {
            "expr": "rate(cache_misses_total[5m])",
            "legendFormat": "Misses",
            "refId": "B"
          }
        ],
        "yaxes": [
          { "format": "ops", "label": "Operations/sec" },
          { "format": "short" }
        ]
      },
      {
        "id": 8,
        "title": "Orders Created (per hour)",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
        "targets": [
          {
            "expr": "increase(orders_created_total[1h])",
            "legendFormat": "{{status}} ({{payment_method}})",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "short", "label": "Orders" },
          { "format": "short" }
        ]
      },
      {
        "id": 9,
        "title": "Cart Abandonment Rate",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 32 },
        "targets": [
          {
            "expr": "rate(cart_abandonments_total[5m])",
            "legendFormat": "{{reason}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "ops", "label": "Abandonments/sec" },
          { "format": "short" }
        ]
      },
      {
        "id": 10,
        "title": "Product Views",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 32 },
        "targets": [
          {
            "expr": "rate(product_views_total[5m])",
            "legendFormat": "Product {{product_id}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "ops", "label": "Views/sec" },
          { "format": "short" }
        ]
      },
      {
        "id": 11,
        "title": "Node.js Memory Usage",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 40 },
        "targets": [
          {
            "expr": "nodejs_heap_size_used_bytes / nodejs_heap_size_total_bytes * 100",
            "legendFormat": "Heap Used %",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "percent", "label": "Memory %" },
          { "format": "short" }
        ]
      },
      {
        "id": 12,
        "title": "Node.js CPU Usage",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 40 },
        "targets": [
          {
            "expr": "rate(nodejs_eventloop_lag_seconds[5m])",
            "legendFormat": "Event Loop Lag",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "s", "label": "Lag" },
          { "format": "short" }
        ]
      }
    ],
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  }
}
